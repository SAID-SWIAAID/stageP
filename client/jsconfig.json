{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/pages/*": ["src/pages/*"],
      "@/services/*": ["src/services/*"],
      "@/context/*": ["src/context/*"],
      "@/lib/*": ["src/lib/*"]
    },
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
